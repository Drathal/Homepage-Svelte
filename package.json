{
  "name": "@drathal/svelte",
  "version": "1.1.0",
  "author": "Markus Dethlefsen",
  "license": "MIT",
  "packageManager": "pnpm@8.6.7",
  "scripts": {
    "dev": "pnpm --parallel -r dev",
    "build": "pnpm --filter components build && pnpm --filter homepage build",
    "build:homepage": "pnpm --filter homepage build",
    "preview": "pnpm --filter homepage preview",
    "test": "pnpm --parallel -r test",
    "check": "pnpm --parallel -r check",
    "check:watch": "pnpm --parallel -r check:watch",
    "test:unit": "pnpm --parallel -r  test:unit",
    "lint": "pnpm --parallel -r lint",
    "clean": "sh ./scripts/clean.sh"
  },
  "devDependencies": {
    "@playwright/test": "^1.36.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "concurrently": "^8.2.0",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-svelte": "^2.32.2",
    "prettier": "^2.8.8",
    "prettier-plugin-svelte": "^2.10.1",
    "tslib": "^2.6.0",
    "typescript": "^5.1.6"
  }
}
